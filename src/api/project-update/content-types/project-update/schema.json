{
  "kind": "collectionType",
  "collectionName": "project_updates",
  "info": {
    "singularName": "project-update",
    "pluralName": "project-updates",
    "displayName": "Project Update",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "projectUpdateTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "projectUpdateIntro": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "maxLength": 288,
      "required": true
    },
    "projectUpdateMainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isFeatured": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project",
      "mappedBy": "projectUpdate"
    },
    "projectUpdateBody": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": true
    }
  }
}
