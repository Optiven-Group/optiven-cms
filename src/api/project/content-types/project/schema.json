{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "projectName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "minLength": 1,
      "default": "Project Name"
    },
    "projectRating": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "default": 1,
      "required": true,
      "max": 5,
      "min": 1
    },
    "projectSummary": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "default": "Project Summary",
      "required": true,
      "minLength": 1,
      "maxLength": 420
    },
    "projectMainBanner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "eighthAcrePrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": false,
      "default": "0",
      "min": "0"
    },
    "quarterAcrePrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "min": "0",
      "required": false,
      "default": "0"
    },
    "halfAcrePrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "min": "0",
      "default": "0",
      "required": false
    },
    "acrePrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "min": "0",
      "required": false,
      "default": "0"
    },
    "isFeatured": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "projectContent": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "sub_projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-project.sub-project",
      "mappedBy": "project"
    },
    "value_additions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::value-addition.value-addition",
      "mappedBy": "project"
    }
  }
}
